{
  "name": "bracketeer",
  "private": true,
  "version": "1.2.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore -A unicorn/no-invalid-fetch-options",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "format": "prettier --write src/",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:webkit": "npx playwright open --browser webkit http://localhost:5173"
  },
  "dependencies": {
    "lodash": "^4.17.23",
    "lodash-es": "^4.17.23",
    "pinia": "^3.0.3",
    "qrcode.vue": "^3.6.0",
    "vue": "^3.5.22",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.7.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/ui": "^4.0.13",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-oxlint": "^1.20.0",
    "eslint-plugin-vue": "^10.5.0",
    "jiti": "^2.6.1",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^1.20.0",
    "prettier": "^3.6.2",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitest": "^4.0.13",
    "vue-tsc": "^3.1.1"
  }
}